<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE configuration PUBLIC
"-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>

	<typeAliases>
		<typeAlias type="com.kh.medic.patient.model.vo.Patient" alias="Patient" />
		<typeAlias type="com.kh.medic.patient.model.vo.Ward" alias="Ward" />
		<typeAlias type="com.kh.medic.member.model.vo.Member" alias="Member" />
		<typeAlias type="com.kh.medic.notice.model.vo.Notice" alias="Notice" />
		<typeAlias type="com.kh.medic.medicine.model.vo.MedicineVo" alias="medicine" />
		<typeAlias type="com.kh.medic.vacation.model.vo.VacationVo" alias="vacation" />
		<typeAlias type="com.kh.medic.empsal.model.vo.EmpsalVo" alias="empsal" />
		<typeAlias type="com.kh.medic.equipment.model.vo.EquipmentVo" alias="equipment" />
		<typeAlias type="com.kh.medic.doctor.model.vo.Medical" alias="Medical" />
		<typeAlias type="com.kh.medic.common.typeHandler.StringArrayTypeHandler" alias="strArrTypeHandler" />
		<typeAlias type="com.kh.medic.common.typeHandler.StringArrayPhoneTypeHandler" alias="strArrPhoneTypeHandler" />
	</typeAliases>
	
	<!-- 어느 부분에 typeHandler를 적용할 것인지 패키지 영역을 설정한다.  -->
	<typeHandlers>
		<package name="com.kh.medic.common.typeHandler" />
	</typeHandlers>
</configuration>










